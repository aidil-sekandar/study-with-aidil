---
const subjectList = [
  { name: "🏃‍♂️ Physics (HL)", url: "physics-hl" },
  { name: "🧮 Math AA (HL)", url: "maa-hl" },
  { name: "🧪 Chemistry (SL)", url: "chemistry-sl" },
  { name: "🧬 Biologi (SPM)", url: "spm-biologi" },
  { name: "📜 Sejarah (SPM)", url: "spm-sejarah" },
];
---

<aside class="h-max grid gap-4 border-[1px] border-[#2C3233] pb-4 rounded-sm">
  <div class="grid">
    <div class="bg-emerald-200/15 pl-4 py-1 rounded-sm mb-3">
      <h3 class="font-bold">📚 Subjects</h3>
    </div>
    <ul class="grid gap-2">
      {
        subjectList.map((subject) => {
          return (
            <li>
              <a class="hover:bg-gray-600/30 block py-1 pl-4" href={`/${subject.url}/`}>
                {subject.name}
              </a>
            </li>
          );
        })
      }
    </ul>
  </div>
